{
  "name": "stockbroker",
  "version": "2.0.0",
  "main": "index.js",
  "repository": "ssh://git@bitbucket.org/MaxSvargal/stockbroker.git",
  "author": "MaxSvargal <maxsvargal@gmail.com>",
  "license": "MIT",
  "scripts": {
    "worker:dev": "gulp",
    "test": "node --experimental-modules ./node_modules/.bin/jest"
  },
  "jest": {
    "transformIgnorePatterns": [
      "./dist/",
      "./node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "mjs"
    ],
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)(spec|test).mjs",
      "**/test/*.mjs"
    ],
    "transform": {
      "^.+\\.mjs$": "babel-jest"
    },
    "env": {
      "type": "node",
      "runner": "node --experimental-modules"
    },
    "automock": false,
    "verbose": true
  },
  "dependencies": {
    "bitfinex-api-node": "^1.2.0",
    "immutable": "^3.8.1",
    "pouchdb": "^6.3.4",
    "redux": "^3.7.2",
    "redux-act": "^1.4.0",
    "redux-pouchdb-plus": "^0.8.1",
    "redux-saga": "^0.15.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.0.2",
    "debug": "^3.0.1",
    "eslint": "^4.7.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "flow-bin": "0.54.1",
    "gulp": "^3.9.1",
    "gulp-file-cache": "^0.0.1",
    "gulp-flow-remove-types": "^1.0.0",
    "gulp-jest": "^2.0.0",
    "gulp-nodemon": "^2.2.1",
    "harmonize": "^2.0.0",
    "jest": "^21.0.2",
    "jest-cli": "^21.1.0",
    "redux-mock-store": "^1.2.3",
    "redux-saga-test-plan": "^3.1.0"
  }
}
