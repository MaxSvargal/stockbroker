    "node-fetch": "^1.6.3",
    "nonce": "^1.0.4",
    "pm2": "^2.4.4",
    "pouchdb": "^6.1.2",
    "pouchdb-adapter-memory": "^6.1.2",
    "react": "^15.4.2",
    "react-chartjs-2": "^2.0.5",
