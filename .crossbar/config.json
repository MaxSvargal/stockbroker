{
  "version": 2,
  "controller": {
    "id": "stockbroker"
  },
  "workers": [
    {
      "type": "router",
      "realms": [
        {
          "name": "maxsvargal_development_worker",
          "roles": [
            {
              "name": "anonymous",
              "permissions": [
                {
                  "uri": "*",
                   "allow": {
                      "call": true,
                      "register": true,
                      "publish": true,
                      "subscribe": true
                   },
                   "cache": true
                }
              ]
            }
          ]
        },
        {
          "name": "maxsvargal_btc_eth",
          "roles": [
            {
              "name": "anonymous",
              "permissions": [
                {
                  "uri": "*",
                   "allow": {
                      "call": true,
                      "register": true,
                      "publish": true,
                      "subscribe": true
                   },
                   "cache": true
                }
              ]
            }
          ]
        },
        {
          "name": "maxsvargal_btc_dash",
          "roles": [
            {
              "name": "anonymous",
              "permissions": [
                {
                 "uri": "*",
                  "allow": {
                     "call": true,
                     "register": true,
                     "publish": true,
                     "subscribe": true
                  },
                  "cache": true
                 }
               ]
             }
           ]
         },
         {
           "name": "lesorub_btc_dash",
           "roles": [
             {
               "name": "anonymous",
               "permissions": [
                 {
                   "uri": "*",
                   "allow": {
                     "call": true,
                     "register": true,
                     "publish": true,
                     "subscribe": true
                   },
                   "cache": true
                 }
               ]
             }
           ]
         },
         {
           "name": "lesorub_btc_fct",
           "roles": [
             {
               "name": "anonymous",
               "permissions": [
                 {
                   "uri": "*",
                   "allow": {
                     "call": true,
                     "register": true,
                     "publish": true,
                     "subscribe": true
                   },
                   "cache": true
                 }
               ]
             }
           ]
         },
         {
           "name": "lesorub_usdt_dash",
           "roles": [
             {
               "name": "anonymous",
               "permissions": [
                 {
                   "uri": "*",
                   "allow": {
                     "call": true,
                     "register": true,
                     "publish": true,
                     "subscribe": true
                   },
                   "cache": true
                 }
               ]
             }
           ]
         },
         {
           "name": "lesorub_btc_ltc",
           "roles": [
             {
               "name": "anonymous",
               "permissions": [
                 {
                   "uri": "*",
                   "allow": {
                     "call": true,
                     "register": true,
                     "publish": true,
                     "subscribe": true
                   },
                   "cache": true
                 }
               ]
             }
           ]
         }
      ],
      "transports": [
        {
          "type": "web",
          "endpoint": {
            "type": "tcp",
            "port": 8080
          },
          "paths": {
            "/": {
              "type": "reverseproxy",
              "host": "localhost",
              "port": 8085
            },
            "ws": {
              "type": "websocket",
              "url": "ws://localhost:8080/ws"
            }
          }
        }
      ]
    }
  ]
}
