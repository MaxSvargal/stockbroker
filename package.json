{
  "name": "stockbroker",
  "version": "1.5.0",
  "main": "index.js",
  "author": "MaxSvargal",
  "license": "MIT",
  "scripts": {
    "build:client": "webpack --config webpack.client.config.js",
    "build:frontend:dev": "rollup -c rollup.frontend.dev.config.js",
    "build:frontend": "rollup -c rollup.frontend.config.js",
    "build:worker:dev": "webpack --config webpack.worker.dev.config.js --watch",
    "build:worker": "rollup -c rollup.worker.config.js",
    "build": "npm run build:worker && npm run build:frontend && npm run build:client",
    "frontend:dev": "npm run build:frontend:dev && NODE_ENV=development node-dev ./dist/frontend_dev_bundle.js",
    "start:worker:dev": "DEBUG=worker,trade NODE_ENV=development ACCOUNT_NAME=maxsvargal ACCOUNT_KEY=123 ACCOUNT_SECRET=345 CURRENCY_PAIR=USDT_ETH DB_NAME=maxsvargal_development_worker node-dev ./dist/worker_dev_bundle.js",
    "worker:dev": "npm-run-all --parallel build:worker:dev start:worker:dev",
    "test": "NODE_ENV=test NODE_PATH=src nyc ava",
    "test:watch": "npm test -- --watch",
    "coverage": "nyc report --reporter=lcov --reporter=text-lcov | coveralls",
    "pm2": "pm2 start ./ecosystem.config.js",
    "start": "npm run build && npm run pm2"
  },
  "ava": {
    "failWithoutAssertions": false,
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "dependencies": {
    "autobahn": "0.12.0",
    "bignumber.js": "^4.0.0",
    "chart.js": "^2.5.0",
    "chokidar": "^1.6.1",
    "crossbar": "^1.0.0",
    "crypto-js": "^3.1.9-1",
    "css-loader": "^0.28.0",
    "debounce": "^1.0.0",
    "deep-equal": "^1.0.1",
    "koa": "^2.2.0",
    "koa-router": "next",
    "koa-static": "^3.0.0",
    "minimist": "^1.2.0",
    "node-fetch": "^1.6.3",
    "nonce": "^1.0.4",
    "npm-run-all": "^4.0.2",
    "pm2": "^2.4.4",
    "pouchdb": "^6.1.2",
    "pouchdb-authentication": "^0.5.5",
    "react": "^15.4.2",
    "react-chartjs-2": "^2.0.5",
    "react-custom-scrollbars": "^4.1.0",
    "react-dom": "^15.4.2",
    "react-grid-layout": "^0.14.6",
    "react-hyperscript-helpers": "^1.2.0",
    "react-redux": "^5.0.3",
    "react-router": "4.1.1",
